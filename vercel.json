{
    "routes": [
        {
            "src": "/",
            "dest": "api/ind.js"
        },
        {
            "src": "api/images",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "/api/images.js"
        },
        {
            "src": "api/upload",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "/api/upload.js"
        },
        {
            "src": "api/delete",
            "methods": [
                "POST",
                "GET",
                "DELETE"
            ],
            "dest": "/api/delete.js",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "https://head.samuelbagin.xyz" },
                { "key": "Access-Control-Allow-Methods", "value": "DELETE" },
                { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
                { "key": "Access-Control-Allow-Credentials", "value": "true" }
              ]
        }
    ]
}